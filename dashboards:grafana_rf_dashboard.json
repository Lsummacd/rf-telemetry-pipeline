      "targets": [
        {
          "datasource": {
            "type": "influxdb",
            "uid": "${DS_INFLUXDB}"
          },
          "query": "// Windowed loss percentage per device_id\nseq = from(bucket: \"rf\")\n  |> range(start: -15m)\n  |> filter(fn: (r) => r._measurement == \"mqtt_consumer\" and r._field == \"seq\")\n  |> filter(fn: (r) => r.device_id =~ /${device:regex}/)\n  |> group(columns: [\"device_id\"])\n  |> sort(columns: [\"_time\"])\n  |> difference(nonNegative: true, columns: [\"_value\"])\n  |> map(fn: (r) => ({\n        r with missing: if r._value > 1.0 then r._value - 1.0 else 0.0\n      }))\n\n// Sum of seq jumps (expected messages = received + missing)\nexpected = seq\n  |> aggregateWindow(every: 1m, fn: sum, createEmpty: false)\n\n// Sum of missing messages\nmissingSum = seq\n  |> map(fn: (r) => ({ r with _value: r.missing }))\n  |> aggregateWindow(every: 1m, fn: sum, createEmpty: false)\n  |> rename(columns: {_value: \"missing_sum\"})\n\n// Join and compute loss %\njoin(tables: {exp: expected, miss: missingSum}, on: [\"_time\",\"device_id\"])\n  |> map(fn: (r) => ({\n        r with _value: if r._value == 0.0 then 0.0 else 100.0 * r.missing_sum / r._value\n      }))\n  |> rename(columns: {_value: \"loss_pct\"})\n  |> keep(columns: [\"_time\",\"device_id\",\"loss_pct\"])\n",
          "queryType": "flux",
          "refId": "A"
        }
      ],
      "title": "Packet Loss (%)",
      "type": "timeseries"
